indxGyro:3 # индекс стоблца начала данных с гироскопа(X, Y, Z)
indxMagnet:6 # индекс стоблца начала данных с магнитометра(X, Y, Z)
indxAcc:0 # индекс стоблца начала данных с акселерометра(X, Y, Z)
indxTime:15 # индекс стоблца начала данных со временем(X, Y, Z)
indxOrient:9 # индекс стоблца начала данных о ориентации(X, Y, Z)
indxGNSS:12 # индекс стоблца начала данных с ГНСС(широта(Latitude), долгота(Longtitude), высота(Alitude))